{
	"product-summary.shelf#home": {
		"children": [
			"stack-layout#prodsum",
			"flex-layout.row#shelfNameAndHeart",
			"flex-layout.row#selling-and-list-price-shelf"
		]
	},

	"stack-layout#prodsum": {
		"children": ["product-summary-image#shelf", "vtex.product-highlights@2.x:product-highlights#collection"]
	},

	"product-summary-image#shelf": {
		"props": {
			"showBadge": true,
			"aspectRatio": "1:1",
			"maxHeight": 120
		}
	},
	"vtex.product-highlights@2.x:product-highlights#collection": {
		"props": {
			"type": "collection"
		},
		"children": ["product-highlight-wrapper"]
	},
	"product-highlight-wrapper": {
		"props": {
			"blockClass": "collection"
		},
		"children": ["product-highlight-text"]
	},
	"product-highlight-text": {
		"props": {
			"message": "{highlightName}"
		}
	},
	"flex-layout.row#selling-price-savings": {
		"props": {
			"colGap": 2,
			"preserveLayoutOnMobile": true,
			"preventHorizontalStretch": true,
			"marginBottom": 20,
			"blockClass": "price-container"
		},
		"children": ["product-selling-price#summary", "product-price-savings#summary"]
	},
	"product-installments#summary": {
		"props": {
			"blockClass": "summary"
		}
	},
	"product-price-savings#summary": {
		"props": {
			"markers": ["discount"],
			"blockClass": "summary"
		}
	},
	"product-summary.shelf#pdp-shelf": {
		"props": {
			"blockClass": "pdp-shelf",
			"priceBehavior": "async"
		},
		"children": [
			"stack-layout#prodsum",
			"flex-layout.row#shelfNameAndHeart",
			"flex-layout.row#selling-and-list-price-shelf"
		]
	},
	"flex-layout.row#add-btn-shelf-pdp-custom-component": {
		"props": {
			"blockClass": "add-to-cart-btn-container-shelf"
		},
		"children": ["add-to-cart-quantity-stepper#shelf"]
	},
	"add-to-cart-quantity-stepper#shelf": {
		"blocks": ["add-to-cart-button#shelf"]
	},
	"flex-layout.row#add-btn-shelf-pdp": {
		"props": {
			"blockClass": "add-to-cart-btn-container-shelf"
		},
		"children": ["add-to-cart-button#shelf"]
	},
	"add-to-cart-button#shelf": {
		"props": {
			"blockClass": "shelfs-add-to-cart-btn",
			"addToCartFeedback": "customEvent",
			"customPixelEventId": "add-to-cart-button",
			"text": "ADICIONAR"
		}
	},
	"add-to-list-btn#heart-pdp": {
		"props": {
			"blockClass": "pdp-wish-heart"
		},
		"title": "Bot√£o de Favorito"
	}
}
